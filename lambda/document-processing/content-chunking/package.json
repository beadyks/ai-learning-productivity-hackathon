{
  "name": "content-chunking",
  "version": "1.0.0",
  "description": "Content chunking and embedding generation Lambda function",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-bedrock-runtime": "^3.400.0",
    "@aws-sdk/client-dynamodb": "^3.400.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "fast-check": "^3.15.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.test.ts",
      "!**/node_modules/**"
    ]
  }
}
